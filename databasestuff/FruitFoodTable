/* CREATE TABLE */
CREATE TABLE fruitfoodinfo (
Food VARCHAR(100),
Serving_Size INTEGER,
Calories INTEGER,
Fat INTEGER,
Sugar INTEGER,
Carbs INTEGER,
Protein INTEGER,
Fiber INTEGER
);

